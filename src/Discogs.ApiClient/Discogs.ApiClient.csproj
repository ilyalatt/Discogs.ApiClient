<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>
  
  <PropertyGroup>
    <Version>0.1.1</Version>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>Discogs.ApiClient</PackageId>
    <PackageProjectUrl>https://github.com/ilyalatt/Discogs.ApiClient</PackageProjectUrl>
    <PackageIconUrl>https://i.ibb.co/vBJsq9H/discogs-vinyl-record-mark.png</PackageIconUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Description>Discogs Api Client</Description>
    <Authors>ilyalatt</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Flurl" Version="2.8.2" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
  </ItemGroup>

</Project>
